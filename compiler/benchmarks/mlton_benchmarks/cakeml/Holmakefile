
all: $(DEFAULT_TARGETS) README.md build-benchmarks
.PHONY: all

README_SOURCES = $(wildcard *Script.sml) $(wildcard *Lib.sml) $(wildcard *Syntax.sml)
DIRS = $(wildcard */)
README.md: $(CAKEMLDIR)/developers/readme_gen readmePrefix $(patsubst %,%readmePrefix,$(DIRS)) $(README_SOURCES)
	$(protect $(CAKEMLDIR)/developers/readme_gen) $(README_SOURCES)

cake.S: $(CAKEMLDIR)/unverified/sexpr-bootstrap/x64/64/cake.S
	$(CP) $(CAKEMLDIR)/unverified/sexpr-bootstrap/x64/64/cake.S cake.S

cake: cake.S
	make -f ../../../bootstrap/compilation/x64/Makefile cake

build-benchmarks: cake basis_ffi.c
	make
.PHONY: build-benchmarks

EXTRA_CLEANS = cake.S cake $(patsubst %.sml,cake_%,$(wildcard *.sml))
