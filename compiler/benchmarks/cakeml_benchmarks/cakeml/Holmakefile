INCLUDES =

all: $(DEFAULT_TARGETS) README.md build-benchmarks
.PHONY: all

README_SOURCES = $(wildcard *Script.sml) $(wildcard *Lib.sml) $(wildcard *Syntax.sml)
DIRS = $(wildcard */)
README.md: $(dprot $(CAKEMLDIR)/developers/readme_gen) readmePrefix $(patsubst %,%readmePrefix,$(DIRS)) $(README_SOURCES)
	$(protect $(CAKEMLDIR)/developers/readme_gen) $(README_SOURCES)

cake.S: $(dprot $(CAKEMLDIR)/unverified/sexpr-bootstrap/x64/64/cake.S)
	$(CP) $< $@

build-benchmarks: cake.S basis_ffi.c
	make
.PHONY: build-benchmarks

EXTRA_CLEANS = cake.S $(patsubst %.sml,cake_%,$(wildcard *.sml))
