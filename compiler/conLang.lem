mat2 : forall ck env e pes err s s'.
evaluate_i2 ck env s e (s', Rerr err)
==>
evaluate_i2 ck env s (Mat_i2 e pes) (s', Rerr err)

and

let1 : forall ck exh genv env n e1 e2 v bv s1 s2.
evaluate_i2 ck (exh,genv,env) s1 e1 (s2, Rval v) &&
