method Main()
{
    print 10 / 3, " ", 10 % 3, "\n";
    print 17 / 5, " ", 17 % 5, "\n";
    print -10 / 3, " ", -10 % 3, "\n";
    print -17 / 5, " ", -17 % 5, "\n";
    print 10 / -3, " ", 10 % -3, "\n";
    print 17 / -5, " ", 17 % -5, "\n";
    print -10 / -3, " ", -10 % -3, "\n";
    print -17 / -5, " ", -17 % -5, "\n";
    print 0 / 3, " ", 0 % 3, "\n";
    print 0 / -3, " ", 0 % -3, "\n";
    print 10 / 1, " ", 10 % 1, "\n";
    print 10 / -1, " ", 10 % -1, "\n";
    print 2 / 3, " ", 2 % 3, "\n";
    print -2 / 3, " ", -2 % 3, "\n";
    print 2 / -3, " ", 2 % -3, "\n";
    print -2 / -3, " ", -2 % -3, "\n";
    print 1 / 3, " ", 1 % 3, "\n";
    print -1 / 3, " ", -1 % 3, "\n";
    print 1 / -3, " ", 1 % -3, "\n";
    print -1 / -3, " ", -1 % -3, "\n";
    print 0 / 1, " ", 0 % 1, "\n";
    print 0 / -1, " ", 0 % -1, "\n";
}
