INCLUDES = $(CAKEMLDIR)/examples/sat_encodings $(CAKEMLDIR)/examples/sat_encodings/case_studies $(CAKEMLDIR)/misc $(CAKEMLDIR)/translator \
            $(CAKEMLDIR)/examples $(HOLDIR)/examples/bootstrap


