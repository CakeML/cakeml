INCLUDES = $(CAKEMLDIR)/examples/deflate $(CAKEMLDIR)/misc $(CAKEMLDIR)/examples/deflate/translation $(CAKEMLDIR)/compiler

all: $(DEFAULT_TARGETS) README.md test
.PHONY: all
README_SOURCES = $(wildcard *Script.sml) $(wildcard *Lib.sml) $(wildcard *Syntax.sml)
DIRS = $(wildcard */)
README.md: $(CAKEMLDIR)/developers/readme_gen readmePrefix $(patsubst %,%readmePrefix,$(DIRS)) $(README_SOURCES)
	$(protect $(CAKEMLDIR)/developers/readme_gen) $(README_SOURCES)

test: testCompileTheory.uo
	gcc -o test test.S $(CAKEMLDIR)/basis/basis_ffi.c
